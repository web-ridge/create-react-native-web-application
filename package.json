{
  "name": "create-react-native-web-application",
  "version": "0.1.1",
  "bin": {
    "create-react-native-web-application": "dist/index.js"
  },
  "description": "create-react-native-web-application is an easy way to get start with react-native-web",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/web-ridge/react-ridge-state",
  "license": "MIT",
  "readme": "README.md",
  "scripts": {
      "type-check": "tsc --noEmit",
      "type-check:watch": "npm run type-check -- --watch",
      "build": "npm run build:types && npm run build:js",
      "build:types": "tsc --emitDeclarationOnly",
      "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "fs-extra": "^9.0.0",
    "node-fs-extra": "^0.8.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^14.0.9",
    "typescript": "^3.9.3"
  }
}
